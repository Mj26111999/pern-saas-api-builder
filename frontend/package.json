{
  "name": "pern-api-builder-frontend",
  "version": "1.0.0",
  "description": "React frontend for PERN Stack SaaS Multi-Tenant API Builder",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.1",
    "react-scripts": "5.0.1",
    "@reduxjs/toolkit": "^1.9.5",
    "react-redux": "^8.1.1",
    "@mui/material": "^5.14.1",
    "@mui/icons-material": "^5.14.1",
    "@mui/lab": "^5.0.0-alpha.136",
    "@mui/x-data-grid": "^6.10.1",
    "@mui/x-date-pickers": "^6.10.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@monaco-editor/react": "^4.5.1",
    "socket.io-client": "^4.7.2",
    "axios": "^1.4.0",
    "react-query": "^3.39.3",
    "react-hook-form": "^7.45.2",
    "yup": "^1.2.0",
    "@hookform/resolvers": "^3.1.1",
    "recharts": "^2.7.2",
    "react-beautiful-dnd": "^13.1.1",
    "react-syntax-highlighter": "^15.5.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-hot-toast": "^2.4.1",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "uuid": "^9.0.0",
    "js-cookie": "^3.0.5",
    "react-helmet-async": "^1.3.0",
    "react-error-boundary": "^4.0.11"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.39",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "eslint": "^8.45.0",
    "eslint-config-react-app": "^7.0.1",
    "prettier": "^3.0.0",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css,md}",
    "analyze": "npm run build && npx bundle-analyzer build/static/js/*.js",
    "docker:build": "docker build -t pern-api-builder-frontend .",
    "docker:run": "docker run -p 3000:80 pern-api-builder-frontend"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "warn",
      "prefer-const": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5000",
  "keywords": [
    "react",
    "api-builder",
    "saas",
    "multi-tenant",
    "pern-stack",
    "material-ui",
    "real-time",
    "websocket"
  ],
  "author": "Michel Jackson <mj26111999@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mj26111999/pern-saas-api-builder.git"
  },
  "bugs": {
    "url": "https://github.com/Mj26111999/pern-saas-api-builder/issues"
  }
}